{
  "name": "poker-hand-history-parser",
  "version": "1.0.0",
  "description": "Parse hand histories from online poker sites",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/poker-apprentice/poker-hand-history-parser",
  "author": "Matt Huggins <matt.huggins@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn build:types && yarn build:grammar && yarn build:webpack",
    "build:types": "tsc -d --emitDeclarationOnly --isolatedModules false --outFile dist/index.d.ts",
    "build:grammar": "antlr4ts -visitor src/grammar/*.g4",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint src",
    "lint:fix": "yarn lint src --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "NODE_ENV=test jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "prettier-2": "npm:prettier@^2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "bignumber.js": "^9.1.2",
    "lodash": "^4.17.21",
    "typescript": "^5.2.2"
  }
}
